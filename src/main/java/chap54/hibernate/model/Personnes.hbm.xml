<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping>
<class name="Personnes" table ="personnes">
    <id name="id" type="int" column="id">
        <generator class="native"/>
    </id>
    <property name="nom" type="string" not-null="true"></property>
    <property name="prenom" type="string" not-null="true"></property>
    <property name="dateNaissance" type="date" column="datenaissance">
        <meta attribute="field-descruption">date de naissance</meta>
    </property>
</class>
</hibernate-mapping>